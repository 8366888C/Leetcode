class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        # Sorting O(nlogn) : n-number of integers
        map = {} # num : count
        output = []
        for num in nums:
            map[num] = map.get(num, 0) + 1
        arr = [[count, num] for num, count in map.items()]
        arr.sort(reverse=True)
        for i in range(k):
            output.append(arr[i][1])
        return output

        # Heap O(nlogk) : n-number of integers, k-top elements
        import heapq
        map = {} # num : count
        for num in nums:
            map[num] = map.get(num, 0) + 1
        arr = []
        for num in map.keys():
            heapq.heappush(arr, (map[num], num))
            if len(arr) > k:
                heapq.heappop(arr)
        output = []
        for i in range(k):
            output.append(heapq.heappop(arr)[1])
        return output

        # Bucket O(n) : n-number of integers
        map = {} # num : count
        for num in nums:
            map[num] = map.get(num, 0) + 1
        buckets = [[] for _ in range(len(nums) + 1)]
        for num, count in map.items():
            buckets[count].append(num)
        output = []
        for count in range(len(buckets) - 1, 0, -1):
            for num in buckets[count]:
                output.append(num)
                if len(output) == k:
                    return output
