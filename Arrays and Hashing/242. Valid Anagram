class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        # Sorting O(nlogn + mlogm)
        if len(s) != len(t):
            return False
        return sorted(s) == sorted(t)

        # Hashmap O(n + m)
        if len(s) != len(t):
            return False
        smap, tmap = {}, {} # letter : count
        for i in s:
            smap[i] = smap.get(i, 0) + 1
        for j in t:
            tmap[j] = tmap.get(j, 0) + 1
        return smap == tmap
