class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        # Sorting O(nlogn + mlogm) : n-length of string(s), m-length of string(t)
        if len(s) != len(t):
            return False
        return sorted(s) == sorted(t)

        # Hashmap O(n + m) : n-number of words(s) m-number of words(t)
        if len(s) != len(t):
            return False
        smap, tmap = {}, {} # letter : count
        for i in s:
            smap[i] = smap.get(i, 0) + 1
        for j in t:
            tmap[j] = tmap.get(j, 0) + 1
        return smap == tmap
