class Solution:
    def checkSubarraySum(self, nums: List[int], k: int) -> bool:
        # Brute Force
        for i in range(len(nums)):
            for j in range(i, len(nums)):
                if (nums[i] + nums[j]) % k == 0 and (nums[i] + nums[j] != k):
                    return True
        return False

        # Hashmap
        curr_sum = 0
        store = {0:-1} # remainder : index
        for i in range(len(nums)):
            curr_sum += nums[i]
            remainder = curr_sum % k
            if remainder in store:
                if i - store[remainder] > 1:
                    return True
            else:
                store[remainder] = i
        return False
