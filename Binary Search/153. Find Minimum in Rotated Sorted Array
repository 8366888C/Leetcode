class Solution:
    def findMin(self, nums: List[int]) -> int:
        # Brute Force O(n) : n-length of array
        nums.sort()
        return nums[0]

        # Binary Search O(logn) : n-length of array
        l, r = 0, len(nums)-1
        while l < r:
            m = l + ((r-l) // 2)
            if nums[m] < nums[r]:
                r = m
            else:
                l = m + 1
        return nums[l]
